# Dial-Craft CRM Frontend Environment Variables
# Development Configuration

# API Configuration
# Note: API calls are proxied through Vite dev server to avoid CORS issues
# VITE_API_URL is not needed in development due to proxy configuration
# VITE_API_URL=http://localhost:3000
VITE_WS_URL=ws://localhost:3000

# Debug Mode
VITE_DEBUG=true

# Application Info
VITE_APP_NAME=Dial-Craft CRM
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_AUDIT_LOGS=true
VITE_ENABLE_BULK_OPERATIONS=true